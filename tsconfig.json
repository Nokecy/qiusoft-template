{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"importHelpers": true,
		"jsx": "react",
		"esModuleInterop": true,
		"sourceMap": true,
		"baseUrl": ".",
		"noImplicitAny": false,
		"strict": true,
		"paths": {
			"@@/*": ["src/.umi/*"],
			"@/*": ["src/*"]
		},
		"allowSyntheticDefaultImports": true
	}
}
